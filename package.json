{
  "name": "jsonfs",
  "version": "1.0.0",
  "description": "A Node.js library for streaming JSON array data from files, with utilities for chunking JSON array elements into manageable sizes.",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
