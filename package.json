    "test": "ts-node tests/index.test.ts"
  },
  "keywords": [
    "json",
    "stream",
    "json array",
    "chunk",
    "batch"
  ],
  "author": "Akash S Panickar",
  "license": "MIT",
  "homepage": "https://github.com/mochatek",
  "repository": {
    "type": "git",
    "url": "https://github.com/mochatek/jsonfs.git"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
